@model IEnumerable<RentalPad.Models.Rooms_reg>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Rental_reg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Room_category)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Rental_reg)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Room_category)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Room_number">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Room_number">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Room_number">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
